{
  "name": "legal-dashboard-api",
  "version": "1.0.0",
  "private": true,
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon --exec ts-node src/app.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "migrate": "ts-node src/utils/migrate.ts",
    "seed": "ts-node src/utils/seed.ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean && npm run lint",
    "postinstall": "npm run build",
    "queue:dev": "ts-node src/queue/worker.ts",
    "db:migrate": "ts-node src/scripts/migrate.ts",
    "db:seed": "ts-node src/scripts/seed.ts",
    "docker:build": "docker build -t legal-dashboard-ts .",
    "docker:run": "docker run -p 3000:3000 legal-dashboard-ts"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.0",
    "cheerio": "^1.0.0-rc.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.12.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.13",
    "pdf-parse": "^1.1.1",
    "redis": "^4.6.13",
    "sharp": "^0.33.4",
    "socket.io": "^4.7.5",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1",
    "ws": "^8.18.0",
    "better-sqlite3": "^9.4.5"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.9",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.14.202",
    "@types/moment": "^2.13.0",
    "@types/morgan": "^1.9.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.11.30",
    "@types/node-cron": "^3.0.11",
    "@types/pdf-parse": "^1.1.5",
    "@types/redis": "^4.0.11",
    "@types/sqlite3": "^3.1.11",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "@types/cheerio": "^0.22.35"
  }
}

