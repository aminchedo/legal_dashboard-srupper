{
  "version": 2,
  "buildCommand": "npm install && npm run build --prefix frontend && pip install -r requirements.txt",
  "outputDirectory": "frontend/dist",
  "framework": null,
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.11"
    }
  },
  "env": {
    "PYTHON_SETUP_REQUIRES": "setuptools>=68.0.0 wheel>=0.41.0",
    "PYTHONPATH": "./"
  }
}